  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#508050",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#90d870"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#b1e39a"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#a6df8c"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#d0e8d0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#d2e9d2"
      }
    },
    {
