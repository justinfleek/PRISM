  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6c6c7e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#cdd6f4"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f2cdcd"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f38ba8"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#94e2d5"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#6c7086"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "scope": "markup.heading.1",
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.2",
      "settings": {
