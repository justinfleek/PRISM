  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#48a078",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#74D2A9"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#b8f0d8"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#98d8c0"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#e8f8f0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c8e8d8"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "entity.name.type",
