  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6b6560",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#d4c4a8"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#b89860"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f0e8e0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c8a870"
      }
    },
    {
