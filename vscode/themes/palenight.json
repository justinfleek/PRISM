  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#676e95",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#c3e88d"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f0e8ff"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#a8a8c0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "scope": "entity.name.tag",
