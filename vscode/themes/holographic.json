  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7888a8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#01cdfe"
      }
    },
    {
      "scope": "string",
