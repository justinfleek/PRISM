  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#1a1f24",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#54aeff"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#b6e3ff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#7ec2ff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#d8e0e7"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#7ec2ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#80ccff"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#3888c8"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7fc2ff"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6dbaff"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#3a424f"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#2088e6"
      }
    },
    {
      "scope": "markup.heading.1",
      "settings": {
        "foreground": "#54aeff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.2",
      "settings": {
