        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.3",
      "settings": {
        "foreground": "#7ec2ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.4",
      "settings": {
        "foreground": "#3888c8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.5",
      "settings": {
        "foreground": "#8898a8",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.heading.6",
      "settings": {
        "foreground": "#1a1f24",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#54aeff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#80ccff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "foreground": "#797f85",
