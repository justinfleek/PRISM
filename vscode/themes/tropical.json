  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#708870",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#E42565"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffd0d8"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ff8090"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f0f8f0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#7fdbca"
      }
    },
    {
