  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#1E483C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#62A527"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ACD322"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#6baa50"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c8e8d0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#8bc34a"
      }
    },
    {
