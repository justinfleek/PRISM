  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#506868",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#00fff0"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ff6b9d"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#d0f0f0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c0a0ff"
      }
    },
    {
      "scope": [
