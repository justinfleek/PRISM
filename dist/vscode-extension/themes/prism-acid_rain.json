  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#4a5d23",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c6ff00"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#9eff6b"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#e5ff4d"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#d4e157"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#a8e600"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#b8e62e"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#d4ff33"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c6ff00"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#9eb83f"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#6b8e23"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#c6ff00"
      }
    },
    {
      "scope": [
        "constant.other.color",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#baff1a"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#a8e600"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#e5ff4d"
      }
    },
    {
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#9eff6b"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#c6ff00"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#ff8585"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#c6ff00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#d4ff33",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#9eff6b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#a8e600"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#baff1a"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#9eb83f"
      }
    }
  ]
}
