  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6a6a68",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#2a9860"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#238151"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#258856"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#1a1a18"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#181816"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#27905b"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#1a1a18"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#238151"
      }
    },
    {
