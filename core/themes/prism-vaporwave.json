  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#a090b0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF3CC7"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#01cdfe"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#f0e0ff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#e8e8f8"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#0ce6e9"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "entity.name.type",
