{
  "name": "prism-themes-cursor",
  "displayName": "PRISM - Where every color has purpose.",
  "description": "64 curated color themes with visual effects. WCAG AA verified color science.",
  "version": "2.0.0",
  "developer": "j-pyxal",
  "publisher": "omegaagentic",
  "author": {
    "name": "Omega Agentic",
    "url": "https://omega.ms"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinfleek/PRISM"
  },
  "bugs": {
    "url": "https://github.com/justinfleek/PRISM/issues"
  },
  "homepage": "https://omega.ms",
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "cursor",
    "color theme",
    "prism",
    "dark theme",
    "light theme",
    "effects",
    "glow",
    "rainbow brackets"
  ],
  "galleryBanner": {
    "color": "#0a0a0a",
    "theme": "dark"
  },
  "icon": "media/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./tsconfig.json",
    "watch": "tsc -watch -p ./tsconfig.json",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.3.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "prism.openEffectsPanel",
        "title": "Prism: Open Effects Panel",
        "icon": "$(symbol-color)"
      },
      {
        "command": "prism.quickPick",
        "title": "Prism: Quick Theme Picker"
      },
      {
        "command": "prism.toggleCursorGlow",
        "title": "Prism: Toggle Cursor Glow"
      },
      {
        "command": "prism.toggleRainbowBrackets",
        "title": "Prism: Toggle Rainbow Brackets"
      }
    ],
    "menus": {
      "commandPalette": [
        { "command": "prism.openEffectsPanel", "group": "prism" },
        { "command": "prism.quickPick", "group": "prism" },
        { "command": "prism.toggleCursorGlow", "group": "prism" },
        { "command": "prism.toggleRainbowBrackets", "group": "prism" }
      ]
    },
    "keybindings": [
      {
        "command": "prism.openEffectsPanel",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p"
      }
    ],
    "configuration": {
      "title": "PRISM Effects",
      "properties": {
        "prism.effects.cursorGlow": {
          "type": "boolean",
          "default": true,
          "description": "Enable subtle glow effect around the cursor"
        },
        "prism.effects.bracketHighlight": {
          "type": "boolean",
          "default": true,
          "description": "Highlight matching brackets on hover"
        },
        "prism.effects.smoothCaret": {
          "type": "boolean",
          "default": true,
          "description": "Enable smooth cursor animation"
        },
        "prism.effects.syntaxPulse": {
          "type": "boolean",
          "default": false,
          "description": "Subtle pulse on syntax errors (experimental)"
        },
        "prism.effects.rainbowBrackets": {
          "type": "boolean",
          "default": false,
          "description": "Enable rainbow bracket colorization"
        },
        "prism.effects.glowIntensity": {
          "type": "number",
          "default": 0.5,
          "minimum": 0,
          "maximum": 1,
          "description": "Intensity of glow effects (0-1)"
        }
      }
    },
    "markdown.previewStyles": [
      "./media/readme-override.css"
    ],
    "themes": [
      { "label": "Prism Acid Rain", "uiTheme": "vs-dark", "path": "./themes/acid_rain.json" },
      { "label": "Prism Amethyst Dusk", "uiTheme": "vs-dark", "path": "./themes/amethyst_dusk.json" },
