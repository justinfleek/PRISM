  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#b0a8a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#e43938"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#c1302f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#cd3332"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#161616"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#131313"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#d83635"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#161616"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c1302f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#131313"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#b0a8a6"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#161616"
      }
    },
    {
      "scope": "markup.heading.1",
      "settings": {
        "foreground": "#e43938",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.2",
      "settings": {
        "foreground": "#f6bfbe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.3",
      "settings": {
        "foreground": "#ed7c7b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.4",
      "settings": {
        "foreground": "#b61918",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.5",
      "settings": {
        "foreground": "#53504f",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.heading.6",
      "settings": {
        "foreground": "#b0a8a6",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#e43938",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#ed7c7b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "foreground": "#625f5e",
