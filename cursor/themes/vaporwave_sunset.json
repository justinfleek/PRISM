  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8060a0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff71ce"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffb8d8"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ffa0c0"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ff90b0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f0e1ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#f0e0ff"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
