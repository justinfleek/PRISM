        "foreground": "#f0e1ff"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#8060a0"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#f0e0ff"
      }
    },
    {
      "scope": "markup.heading.1",
      "settings": {
        "foreground": "#ff71ce",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.2",
      "settings": {
