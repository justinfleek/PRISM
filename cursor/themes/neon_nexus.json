  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#308830",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ff8800"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#88ffff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#ff00aa"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#88ffff"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#308830"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#ff00aa"
      }
    },
    {
      "scope": "markup.heading.1",
      "settings": {
        "foreground": "#ff00aa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.2",
      "settings": {
        "foreground": "#ff99fe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.3",
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.4",
      "settings": {
        "foreground": "#b200b2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading.5",
      "settings": {
        "foreground": "#90cf90",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.heading.6",
      "settings": {
        "foreground": "#308830",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#fe4cfe",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "foreground": "#80c380",
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
