        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#dfbfff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#cccac2"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f29e74"
      }
    },
