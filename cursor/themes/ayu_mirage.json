  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5c6773",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ffa759"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#d5ff80"
      }
    },
    {
