{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Ayu Mirage",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Ayu Mirage",
      "appearance": "#73d0ff",
      "style": {
        "background": "#1f2430",
        "editor.background": "#1f2430",
        "editor.foreground": "#cccac2",
        "editor.gutter.background": "#1f2430",
        "editor.line_highlight.background": "#242936",
        "editor.active_line_number": "#cccac2",
        "editor.wrap_guide": "#5c6773",
        "terminal.background": "#1f2430",
        "terminal.foreground": "#cccac2",
        "terminal.ansi.black": "#1f2430",
        "terminal.ansi.red": "#d8ad56",
        "terminal.ansi.green": "#ffd175",
        "terminal.ansi.yellow": "#7c858f",
        "terminal.ansi.blue": "#ffcc66",
        "terminal.ansi.magenta": "#bf994c",
        "terminal.ansi.cyan": "#ffd684",
        "terminal.ansi.white": "#cccac2",
        "syntax": {
          "comment": {
            "color": "#5c6773",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ffa759",
            "font_weight": 600
          },
          "string": {
            "color": "#d5ff80"
          },
          "function": {
            "color": "#ffd580"
          },
          "number": {
            "color": "#dfbfff"
          },
          "type": {
            "color": "#73d0ff"
          },
          "variable": {
            "color": "#cccac2"
          },
          "constant": {
            "color": "#dfbfff"
          },
          "attribute": {
            "color": "#d4bfff"
          },
          "property": {
            "color": "#cccac2"
          },
          "punctuation": {
            "color": "#cccac2"
