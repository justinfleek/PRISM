        "editor.active_line_number": "#bf616a",
        "editor.wrap_guide": "#6878a8",
        "terminal.background": "#2e3440",
        "terminal.foreground": "#bf616a",
        "terminal.ansi.black": "#2e3440",
        "terminal.ansi.red": "#6d88a4",
        "terminal.ansi.green": "#8daac7",
        "terminal.ansi.yellow": "#9fccd9",
        "terminal.ansi.blue": "#81a1c1",
