{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Minimal",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Minimal",
      "appearance": "#161616",
      "style": {
        "background": "#f5f5f5",
        "editor.background": "#f5f5f5",
        "editor.foreground": "#161616",
        "editor.gutter.background": "#f5f5f5",
        "editor.line_highlight.background": "#ebebeb",
        "editor.active_line_number": "#161616",
        "editor.wrap_guide": "#b0a8a6",
        "terminal.background": "#f5f5f5",
        "terminal.foreground": "#161616",
        "terminal.ansi.black": "#161616",
        "terminal.ansi.red": "#c1302f",
        "terminal.ansi.green": "#e64c4b",
        "terminal.ansi.yellow": "#b0a8a6",
        "terminal.ansi.blue": "#e43938",
        "terminal.ansi.magenta": "#ab2a2a",
        "terminal.ansi.cyan": "#e85655",
        "terminal.ansi.white": "#f5f5f5",
        "syntax": {
          "comment": {
            "color": "#b0a8a6",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#e43938",
            "font_weight": 600
          },
          "string": {
            "color": "#c1302f"
          },
          "function": {
            "color": "#d83635"
          },
          "number": {
            "color": "#cd3332"
          },
          "type": {
            "color": "#161616"
          },
          "variable": {
            "color": "#161616"
          },
          "constant": {
            "color": "#cd3332"
          },
          "attribute": {
            "color": "#131313"
          },
          "property": {
            "color": "#161616"
          },
          "punctuation": {
            "color": "#161616"
          },
          "operator": {
            "color": "#e43938"
          }
        }
      }
    }
  ]
}