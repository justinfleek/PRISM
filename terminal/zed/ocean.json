          "function": {
            "color": "#ffffff"
          },
          "number": {
            "color": "#68c8d0"
          },
          "type": {
            "color": "#88a8a8"
          },
          "variable": {
            "color": "#e0f0f0"
          },
          "constant": {
            "color": "#68c8d0"
          },
          "attribute": {
            "color": "#52aeb1"
          },
          "property": {
            "color": "#e0f0f0"
          },
          "punctuation": {
            "color": "#348FD5"
