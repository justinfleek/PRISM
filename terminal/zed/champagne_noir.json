{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Champagne Noir",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Champagne Noir",
      "appearance": "#a8a090",
      "style": {
        "background": "#0a0a08",
        "editor.background": "#0a0a08",
        "editor.foreground": "#f0ece0",
        "editor.gutter.background": "#0a0a08",
        "editor.line_highlight.background": "#121210",
        "editor.active_line_number": "#f0ece0",
        "editor.wrap_guide": "#6b6560",
        "terminal.background": "#0a0a08",
        "terminal.foreground": "#f0ece0",
        "terminal.ansi.black": "#0a0a08",
        "terminal.ansi.red": "#d1c4af",
        "terminal.ansi.green": "#f7e9d2",
        "terminal.ansi.yellow": "#7b7b77",
        "terminal.ansi.blue": "#f7e7ce",
        "terminal.ansi.magenta": "#b9ad9a",
        "terminal.ansi.cyan": "#f8ebd7",
        "terminal.ansi.white": "#f0ece0",
        "syntax": {
          "comment": {
            "color": "#6b6560",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ffffff",
            "font_weight": 600
          },
          "string": {
            "color": "#d4c4a8"
          },
          "function": {
            "color": "#e8d8b8"
          },
          "number": {
            "color": "#b89860"
          },
          "type": {
            "color": "#a8a090"
          },
          "variable": {
            "color": "#f0e8e0"
          },
          "constant": {
            "color": "#b89860"
          },
          "attribute": {
            "color": "#c0a880"
          },
          "property": {
            "color": "#f0e8e0"
          },
          "punctuation": {
            "color": "#f0ece0"
          },
          "operator": {
            "color": "#ffffff"
          }
        }
      }
    }
  ]
}