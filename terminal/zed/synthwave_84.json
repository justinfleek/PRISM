{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Synthwave 84",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Synthwave 84",
      "appearance": "#ff8b39",
      "style": {
        "background": "#262335",
        "editor.background": "#262335",
        "editor.foreground": "#ff8b39",
        "editor.gutter.background": "#262335",
        "editor.line_highlight.background": "#302a42",
        "editor.active_line_number": "#ff8b39",
        "editor.wrap_guide": "#614d73",
        "terminal.background": "#262335",
        "terminal.foreground": "#ff8b39",
        "terminal.ansi.black": "#262335",
        "terminal.ansi.red": "#2dd3d1",
        "terminal.ansi.green": "#4af9f6",
        "terminal.ansi.yellow": "#ff97e2",
        "terminal.ansi.blue": "#36f9f6",
        "terminal.ansi.magenta": "#28bab8",
        "terminal.ansi.cyan": "#5efaf7",
        "terminal.ansi.white": "#ff8b39",
        "syntax": {
          "comment": {
            "color": "#614d73",
            "font_style": "italic"
