{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Night Owl",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Night Owl",
      "appearance": "#ffcb8b",
      "style": {
        "background": "#011627",
        "editor.background": "#011627",
        "editor.foreground": "#d6deeb",
        "editor.gutter.background": "#011627",
        "editor.line_highlight.background": "#051e32",
        "editor.active_line_number": "#d6deeb",
        "editor.wrap_guide": "#82aaff",
        "terminal.background": "#011627",
        "terminal.foreground": "#d6deeb",
        "terminal.ansi.black": "#011627",
        "terminal.ansi.red": "#a97cc6",
        "terminal.ansi.green": "#cc9cec",
        "terminal.ansi.yellow": "#9bbbff",
        "terminal.ansi.blue": "#c792ea",
        "terminal.ansi.magenta": "#956daf",
        "terminal.ansi.cyan": "#d2a7ee",
        "terminal.ansi.white": "#f78c6c",
        "syntax": {
          "comment": {
            "color": "#82aaff",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#c792ea",
            "font_weight": 600
          },
          "string": {
            "color": "#ecc48d"
          },
          "function": {
            "color": "#82aaff"
          },
          "number": {
            "color": "#ff5874"
          },
          "type": {
            "color": "#ffcb8b"
          },
          "variable": {
            "color": "#d6deeb"
          },
          "constant": {
            "color": "#ff5874"
          },
          "attribute": {
            "color": "#cfa2ed"
          },
          "property": {
            "color": "#d6deeb"
          },
          "punctuation": {
            "color": "#d6deeb"
          },
          "operator": {
            "color": "#c792ea"
          }
        }
      }
    }
  ]
}