{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Tropical",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Tropical",
      "appearance": "#f0b880",
      "style": {
        "background": "#0a1810",
        "editor.background": "#0a1810",
        "editor.foreground": "#f0f8f0",
        "editor.gutter.background": "#0a1810",
        "editor.line_highlight.background": "#0f8a63",
        "editor.active_line_number": "#f0f8f0",
        "editor.wrap_guide": "#708870",
        "terminal.background": "#0a1810",
        "terminal.foreground": "#f0f8f0",
        "terminal.ansi.black": "#0B6E4F",
        "terminal.ansi.red": "#c11f55",
        "terminal.ansi.green": "#e63a74",
        "terminal.ansi.yellow": "#ce3a7b",
        "terminal.ansi.blue": "#E42565",
        "terminal.ansi.magenta": "#ab1b4b",
        "terminal.ansi.cyan": "#e95083",
        "terminal.ansi.white": "#D9E76C",
        "syntax": {
          "comment": {
            "color": "#708870",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#E42565",
            "font_weight": 600
          },
          "string": {
            "color": "#ffd0d8"
          },
          "function": {
            "color": "#ffffff"
          },
          "number": {
            "color": "#ff8090"
          },
          "type": {
            "color": "#f0b880"
          },
          "variable": {
            "color": "#f0f8f0"
          },
          "constant": {
            "color": "#ff8090"
          },
          "attribute": {
            "color": "#e8457c"
          },
          "property": {
            "color": "#f0f8f0"
          },
          "punctuation": {
            "color": "#f0f8f0"
          },
          "operator": {
            "color": "#E42565"
          }
        }
      }
    }
  ]
}