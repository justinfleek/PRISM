          },
          "string": {
            "color": "#3ad900"
          },
          "function": {
            "color": "#ffc600"
          },
          "number": {
            "color": "#ff80e1"
          },
          "type": {
            "color": "#80ffbb"
          },
          "variable": {
            "color": "#0088ff"
          },
          "constant": {
            "color": "#ff80e1"
          },
          "attribute": {
            "color": "#9effff"
          },
          "property": {
            "color": "#0088ff"
          },
          "punctuation": {
            "color": "#80ffbb"
          },
          "operator": {
