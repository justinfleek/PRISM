{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Midnight Sapphire",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Midnight Sapphire",
      "appearance": "#4070a0",
      "style": {
        "background": "#050810",
        "editor.background": "#050810",
        "editor.foreground": "#e0e8f8",
        "editor.gutter.background": "#050810",
        "editor.line_highlight.background": "#0a1018",
        "editor.active_line_number": "#e0e8f8",
        "editor.wrap_guide": "#606878",
        "terminal.background": "#050810",
        "terminal.foreground": "#e0e8f8",
        "terminal.ansi.black": "#050810",
        "terminal.ansi.red": "#a3a3b0",
        "terminal.ansi.green": "#c6c6d4",
        "terminal.ansi.yellow": "#6e7786",
        "terminal.ansi.blue": "#c0c0d0",
        "terminal.ansi.magenta": "#90909c",
        "terminal.ansi.cyan": "#ccccd9",
        "terminal.ansi.white": "#e0e8f8",
        "syntax": {
          "comment": {
            "color": "#606878",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#6090d0",
            "font_weight": 600
          },
          "string": {
            "color": "#c0d0e8"
          },
          "function": {
            "color": "#f0f4f8"
          },
          "number": {
            "color": "#88b0e0"
          },
          "type": {
            "color": "#4070a0"
          },
          "variable": {
            "color": "#d0d8e8"
          },
          "constant": {
            "color": "#88b0e0"
          },
          "attribute": {
            "color": "#e1e9f8"
          },
          "property": {
            "color": "#d0d8e8"
          },
          "punctuation": {
            "color": "#e0e8f8"
          },
          "operator": {
            "color": "#6090d0"
          }
        }
      }
    }
  ]
}