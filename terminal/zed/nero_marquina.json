        "editor.line_highlight.background": "#101014",
        "editor.active_line_number": "#e8e4dc",
        "editor.wrap_guide": "#5a5a68",
        "terminal.background": "#08080a",
        "terminal.foreground": "#e8e4dc",
        "terminal.ansi.black": "#08080a",
        "terminal.ansi.red": "#b4942e",
        "terminal.ansi.green": "#d8b74b",
        "terminal.ansi.yellow": "#7b7b86",
        "terminal.ansi.blue": "#d4af37",
        "terminal.ansi.magenta": "#9f8329",
