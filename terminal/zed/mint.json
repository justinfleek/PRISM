{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Mint",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Mint",
      "appearance": "#88b8a8",
      "style": {
        "background": "#0a1818",
        "editor.background": "#0a1818",
        "editor.foreground": "#e8f8f0",
        "editor.gutter.background": "#0a1818",
        "editor.line_highlight.background": "#0d2020",
        "editor.active_line_number": "#e8f8f0",
        "editor.wrap_guide": "#48a078",
        "terminal.background": "#0a1818",
        "terminal.foreground": "#e8f8f0",
        "terminal.ansi.black": "#0d1e1e",
        "terminal.ansi.red": "#62b28f",
        "terminal.ansi.green": "#81d6b1",
        "terminal.ansi.yellow": "#A6F2B8",
        "terminal.ansi.blue": "#74D2A9",
        "terminal.ansi.magenta": "#579d7e",
        "terminal.ansi.cyan": "#88d8b5",
        "terminal.ansi.white": "#EDFDEE",
        "syntax": {
          "comment": {
            "color": "#48a078",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#74D2A9",
            "font_weight": 600
          },
          "string": {
            "color": "#b8f0d8"
          },
          "function": {
            "color": "#ffffff"
          },
          "number": {
            "color": "#98d8c0"
          },
          "type": {
            "color": "#88b8a8"
          },
          "variable": {
            "color": "#e8f8f0"
          },
          "constant": {
            "color": "#98d8c0"
          },
          "attribute": {
            "color": "#68bd98"
          },
          "property": {
            "color": "#e8f8f0"
          },
          "punctuation": {
            "color": "#e8f8f0"
          },
          "operator": {
            "color": "#74D2A9"
          }
        }
      }
    }
  ]
}