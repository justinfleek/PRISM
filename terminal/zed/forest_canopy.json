{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Forest Canopy",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Forest Canopy",
      "appearance": "#d0e8d0",
      "style": {
        "background": "#0a100a",
        "editor.background": "#0a100a",
        "editor.foreground": "#d0e8d0",
        "editor.gutter.background": "#0a100a",
        "editor.line_highlight.background": "#101810",
        "editor.active_line_number": "#d0e8d0",
        "editor.wrap_guide": "#508050",
        "terminal.background": "#0a100a",
        "terminal.foreground": "#d0e8d0",
        "terminal.ansi.black": "#0a100a",
        "terminal.ansi.red": "#7ab75f",
        "terminal.ansi.green": "#9bdb7e",
        "terminal.ansi.yellow": "#739973",
        "terminal.ansi.blue": "#90d870",
        "terminal.ansi.magenta": "#6ca254",
        "terminal.ansi.cyan": "#a6df8c",
        "terminal.ansi.white": "#d0e8d0",
        "syntax": {
          "comment": {
            "color": "#508050",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#90d870",
            "font_weight": 600
          },
          "string": {
            "color": "#b1e39a"
          },
          "function": {
            "color": "#abe193"
          },
          "number": {
            "color": "#a6df8c"
          },
          "type": {
            "color": "#d0e8d0"
          },
          "variable": {
            "color": "#d0e8d0"
          },
          "constant": {
            "color": "#a6df8c"
          },
          "attribute": {
            "color": "#d2e9d2"
          },
          "property": {
            "color": "#d0e8d0"
          },
          "punctuation": {
            "color": "#d0e8d0"
          },
          "operator": {
            "color": "#90d870"
          }
        }
      }
    }
  ]
}