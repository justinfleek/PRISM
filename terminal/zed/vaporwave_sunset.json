          "function": {
            "color": "#00ffff"
          },
          "number": {
            "color": "#ffa0c0"
          },
          "type": {
            "color": "#f0e0ff"
          },
          "variable": {
            "color": "#ff90b0"
          },
          "constant": {
            "color": "#ffa0c0"
          },
          "attribute": {
            "color": "#f0e1ff"
          },
          "property": {
            "color": "#ff90b0"
          },
          "punctuation": {
            "color": "#f0e0ff"
