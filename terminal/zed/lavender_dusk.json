{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Lavender Dusk",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Lavender Dusk",
      "appearance": "#e0d0f0",
      "style": {
        "background": "#10081a",
        "editor.background": "#10081a",
        "editor.foreground": "#e0d0f0",
        "editor.gutter.background": "#10081a",
        "editor.line_highlight.background": "#181020",
        "editor.active_line_number": "#e0d0f0",
        "editor.wrap_guide": "#705088",
        "terminal.background": "#10081a",
        "terminal.foreground": "#e0d0f0",
        "terminal.ansi.black": "#10081a",
        "terminal.ansi.red": "#9873d8",
        "terminal.ansi.green": "#ba93ff",
        "terminal.ansi.yellow": "#8c739f",
        "terminal.ansi.blue": "#b388ff",
        "terminal.ansi.magenta": "#8666bf",
        "terminal.ansi.cyan": "#c29fff",
        "terminal.ansi.white": "#e0d0f0",
        "syntax": {
          "comment": {
            "color": "#705088",
            "font_style": "italic"
