          "number": {
            "color": "#ff9e64"
          },
          "type": {
            "color": "#2ac3de"
          },
          "variable": {
            "color": "#7aa2f7"
          },
          "constant": {
            "color": "#ff9e64"
          },
          "attribute": {
            "color": "#c5a9f8"
          },
          "property": {
            "color": "#7aa2f7"
