          "function": {
            "color": "#ffffff"
          },
          "number": {
            "color": "#d09098"
          },
          "type": {
            "color": "#fe4c6d"
          },
          "variable": {
            "color": "#f0e0e0"
          },
          "constant": {
            "color": "#d09098"
          },
          "attribute": {
            "color": "#ebbfc2"
          },
          "property": {
            "color": "#f0e0e0"
          },
          "punctuation": {
            "color": "#f0e8e8"
