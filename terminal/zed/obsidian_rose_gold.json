{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Obsidian Rose Gold",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Obsidian Rose Gold",
      "appearance": "#fe4c6d",
      "style": {
        "background": "#050404",
        "editor.background": "#050404",
        "editor.foreground": "#f0e8e8",
        "editor.gutter.background": "#050404",
        "editor.line_highlight.background": "#0c0a0a",
        "editor.active_line_number": "#f0e8e8",
        "editor.wrap_guide": "#a89898",
        "terminal.background": "#050404",
        "terminal.foreground": "#f0e8e8",
        "terminal.ansi.black": "#050404",
        "terminal.ansi.red": "#c5999c",
        "terminal.ansi.green": "#eabbbf",
        "terminal.ansi.yellow": "#7b7777",
        "terminal.ansi.blue": "#e8b4b8",
        "terminal.ansi.magenta": "#ae878a",
        "terminal.ansi.cyan": "#ecc3c6",
        "terminal.ansi.white": "#f0e8e8",
        "syntax": {
          "comment": {
            "color": "#a89898",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#e8b4b8",
            "font_weight": 600
          },
          "string": {
            "color": "#ffeedd"
