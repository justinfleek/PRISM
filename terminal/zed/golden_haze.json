{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism Golden Haze",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism Golden Haze",
      "appearance": "#1a1c20",
      "style": {
        "background": "#e8eaee",
        "editor.background": "#e8eaee",
        "editor.foreground": "#1a1c20",
        "editor.gutter.background": "#e8eaee",
        "editor.line_highlight.background": "#e8eaee",
        "editor.active_line_number": "#1a1c20",
        "editor.wrap_guide": "#585048",
        "terminal.background": "#e8eaee",
        "terminal.foreground": "#1a1c20",
        "terminal.ansi.black": "#1a1c20",
        "terminal.ansi.red": "#ab7503",
        "terminal.ansi.green": "#cf951d",
        "terminal.ansi.yellow": "#707478",
        "terminal.ansi.blue": "#ca8a04",
        "terminal.ansi.magenta": "#976703",
        "terminal.ansi.cyan": "#d4a136",
        "terminal.ansi.white": "#e8eaee",
        "syntax": {
          "comment": {
            "color": "#585048",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ca8a04",
