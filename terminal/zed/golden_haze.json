          },
          "string": {
            "color": "#888070"
          },
          "function": {
            "color": "#e8b840"
          },
          "number": {
            "color": "#906000"
          },
          "type": {
            "color": "#1a1c20"
          },
          "variable": {
            "color": "#1a1c20"
          },
          "constant": {
            "color": "#906000"
          },
          "attribute": {
            "color": "#181a1e"
          },
          "property": {
            "color": "#1a1c20"
          },
          "punctuation": {
            "color": "#1a1c20"
